<launch>

    <arg name="use_gui" default="true"/>
    
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="gui" value="$(arg use_gui)"/>
    </include>

    <!-- Spawn the robot -->
    <include file="$(find description)/launch/spawn.launch">
        <arg name="robot_ns" value="photon"/>
    </include>

</launch>

